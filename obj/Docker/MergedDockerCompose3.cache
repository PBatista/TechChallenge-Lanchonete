name: dockercompose15676443681506708962
services:
  api:
    build:
      context: C:\Users\peeeh\OneDrive\Documentos\FIAP\TECH_CHALLENGER\Projeto\Lanchonete
      dockerfile: API/Dockerfile
    depends_on:
      mongo:
        condition: service_started
        required: true
    entrypoint:
      - ./wait-for-it.sh
      - mongodb-container:27018
      - --
      - npm
      - start
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
    image: api
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
  mongo:
    command:
      - mongod
      - --port
      - "27018"
      - --bind_ip_all
    container_name: mongodb-container
    environment:
      MONGO_INITDB_DATABASE: lanchonete
    image: mongo:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27018
        published: "27018"
        protocol: tcp
    volumes:
      - type: volume
        source: mongodb-data
        target: /data/db
        volume: {}
networks:
  default:
    name: dockercompose15676443681506708962_default
volumes:
  mongodb-data:
    name: dockercompose15676443681506708962_mongodb-data
    driver: local